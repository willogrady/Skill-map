  CONSTRAINT `rolesibfk_1` FOREIGN KEY (`role_group_id`) REFERENCES `role_group` (`role_group_id`)) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8;
