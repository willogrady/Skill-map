CREATE TABLE `svcrolesdb`.`role` (
  `role_id` INT NOT NULL AUTO_INCREMENT,
  `role_title` VARCHAR(45) NOT NULL,
  `role_grade` VARCHAR(30) NOT NULL,
  `role_summary` VARCHAR(400) NOT NULL,
  `version_id` INT NOT NULL,
